{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.company.com/brandNew.schema.json",
    "type": "object",
    "properties": {
      "handle": {
        "type": "string",
        "minLength": 1
      },
      "name": {
        "type": "string",
        "minLength": 1
      },
      "location": {
        "type": "string",
        "minLength": 1
      },
      "logoUrl": {
        "type": "string",
        "minLength": 1
      }
    },
    "additionalProperties": false,
    "required": [
      "handle",
      "name"
    ]
  }